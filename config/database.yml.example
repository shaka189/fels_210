default: &default
  adapter: mysql2
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>
  timeout: 5000
  username: <%= ENV["USERNAME"] %>
  password: <%= ENV["PASSWORD"] %>
development:
  <<: *default
  database: <%= ENV["DEVELOPMENT"] %>


test:
  <<: *default
  database: <%= ENV["TEST"] %>

production:
  <<: *default
  database: <%= ENV["PRODUCTION"] %>
